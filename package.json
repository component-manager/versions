{
  "name": "husky2",
  "version": "0.0.4-pre.1",
  "description": "Modern native Git hooks made easy",
  "keywords": [
    "git",
    "hooks",
    "pre-commit"
  ],
  "homepage": "https://github.com/lemanschik/husky2",
  "repository": "lemanschik/husky2",
  "bin": "husky2.bin.js",
  "files": [
    "husky2.js",
    "husky2.bin.js"
  ],
  "noScripts": {
    "build": "tsc",
    "test": "sh test/all.sh",
    "lint": "eslint src --ext .ts",
    "serve": "docsify serve docs",
    "prepare": "npm run build && node lib/bin install"
  },
  "noDevDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@tsconfig/node18": "^1.0.3",
    "@types/node": "^18.11.18",
    "@typicode/eslint-config": "^1.1.0",
    "docsify-cli": "^4.4.4",
    "typescript": "^4.9.4"
  },
  "noEngines": {
    "node": ">=17"
  }
}
